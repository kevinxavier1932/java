import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        //int real=a;
        int rev=0;
        while(a>0){
          int  rev1=a%10;
            rev=rev*10+rev1;
           a=a/10;
        }
        if(a==rev){
            System.out.print("Palindrome");
        }
        else{
            System.out.print("Not Palindrome");
        }

    }
}
