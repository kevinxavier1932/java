import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
    Scanner scan=new Scanner(System.in);
        int a=scan.nextInt();
        int sum=0;
        for(int i=0;i<a;i++)
        {
            int b=scan.nextInt();
            sum=sum+b;
            }  
        System.out.print(sum);
    
    }
}
