import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int d=0;
        while(n>0){
            int digit=n%10;
            d=d*10+digit;
            n=n/10;
        }
          System.out.println(d);
          }
    }
